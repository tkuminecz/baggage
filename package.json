{
  "name": "oss",
  "version": "1.0.0",
  "description": "A monorepo for my oss packages",
  "bugs": {
    "url": "https://github.com/tkuminecz/oss/issues"
  },
  "homepage": "https://github.com/tkuminecz/oss#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tkuminecz/oss.git"
  },
  "author": "Tim Kuminecz <tkuminecz@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck"
  },
  "turbo": {
    "pipeline": {
      "build": {},
      "lint": {
        "outputs": []
      },
      "test": {
        "dependsOn": [
          "^build"
        ],
        "outputs": []
      },
      "typecheck": {
        "dependsOn": [
          "^build"
        ],
        "outputs": []
      }
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~5.9.1",
    "@typescript-eslint/parser": "~5.9.1",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~25.7.0",
    "eslint-plugin-prettier": "~4.0.0",
    "jest": "~27.4.7",
    "prettier": "~2.5.1",
    "turbo": "~1.0.28",
    "typescript": "~4.5.4"
  }
}
